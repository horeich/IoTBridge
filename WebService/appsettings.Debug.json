{
	"KeyVault": {
		"Name": "${PCS_KEYVAULT_NAME}",
		"AADClientId": "${PCS_AAD_APPID}",
		"AADClientSecret": "${PCS_AAD_APPSECRET}"
	},
	"Logging": {
		"MinimumLogLevel": "Trace"
	},
	"AllowedHosts": "*",
	"IoTBridge": {
		"Name": "IoTBridge",
		"webServicePort": "9021"
	},
	"StorageAdapter": {
		"WebServiceUrl": "http://localhost:9022/v1",
		"WebServiceTimeout": 10000,
		"DevicesContainerId": "devices",
		"MappingsContainerId": "mappings",
		"DocumentId": "devices"
	},
	"DeviceClient": {
		"ConnectionTimeout": 120000
	}
}