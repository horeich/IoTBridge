{
	"KeyVault": {
		"Name": "${PCS_KEYVAULT_NAME}",
		"AADClientId": "${PCS_AAD_APPID}",
		"AADClientSecret": "${PCS_AAD_APPSECRET}"
	},
	"Logging": {
		"LogLevel": "Debug"
	},
	"AllowedHosts": "*",
	"IoTBridge": {
		"Name": "IoTBridge",
		"DeviceUpdateInterval": 20,
		"webServicePort": "9021"
	},
	"StorageAdapter": {
		"WebServiceUrl": "http://localhost:9022/v1",
		"WebServiceTimeout": 10000,
		"DeviceCollectionId": "device",
		"MappingCollectionId": "mapping",
		"DocumentId": "devices"
	},
	"IoTHub": {
		"TelemetryTimeout": 10000
	},
	"Quartz": {
		"quartz.scheduler.instanceName": "IoTBridgeScheduler"
	}
}